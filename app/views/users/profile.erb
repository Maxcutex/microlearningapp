                    <div class="col-sm-10">
                      <input name="user[first_name]"  type="text" class="form-control" id="first_name"  placeholder="First Name" value="<%=current_user.first_name%>">
                    </div>
                  </div> 
                  <div class="form-group">
                    <label for="last_name" class="col-sm-2 control-label">Last Name</label>

